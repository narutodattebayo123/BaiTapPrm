<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <ImageView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:id="@+id/demo91_itemview_searchImage"
        android:src="@drawable/android"
        tools:ignore="MissingConstraints" />
    <TextView
        android:layout_marginRight="30dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/demo91_itemview_TvStyleID"
        android:text="TvStyleID"/>
    <TextView
        android:layout_marginRight="30dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/demo91_itemview_TvBrand"
        android:text="TvBrand" />
    <TextView
        android:layout_marginRight="30dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/demo91_itemview_TvPrice"
        android:text="TvPrice"/>
    <TextView
        android:layout_marginRight="30dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/demo91_itemview_TvInfo"
        android:text="TvInfo"/>
</androidx.constraintlayout.widget.ConstraintLayout>